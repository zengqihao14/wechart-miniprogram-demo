.components-container {
    box-sizing: border-box;
    padding: 16px;
    display: flex;
    flex-direction: column;
    justify-content: start;
    min-height: 100vh;
}
.components-title {
    box-sizing: border-box;
    width: 100%;
    font-size: 24px;
    font-weight: 500;
    text-align: center;
    margin: 16px auto 32px;
}